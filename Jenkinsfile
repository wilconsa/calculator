pipeline {
    agent any
    stages {
       stage("build"){
          steps{
            echo 'Contruye la aplicacion'
          }
        }
        stage("test"){
          steps{
            echo 'Testear la aplicacion'
          }
        }
        stage("deploy"){
          steps{
            echo 'Desplegare la aplicacion'
          }
        }
       stage("Finalizar"){
          steps{
            echo 'Finalizare la aplicacion....6'
          }
        }
    }
}
